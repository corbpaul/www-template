{
  "name": "www-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:corbpaul/www-template.git",
  "author": "Paul Corbett <3407807+corbpaul@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start:server": "NODE_ENV=development nodemon -r tsconfig-paths/register src/server/index.ts",
    "test:lint:fix": "eslint . --fix",
    "test:lint": "eslint .",
    "test:static": "yarn test:lint && yarn test:types",
    "test:types:coverage": "typescript-coverage-report",
    "test:types": "type-coverage",
    "test:unit:coverage": "jest --coverage --passWithNoTests",
    "test:unit": "jest --passWithNoTests",
    "test": "yarn test:static && yarn test:unit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.0",
    "@types/compression": "1.7.0",
    "@types/dotenv-safe": "8.1.1",
    "@types/express": "4.17.12",
    "@types/jest": "26.0.23",
    "@types/jest-axe": "3.5.1",
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-axe": "5.0.1",
    "jest-transform-graphql": "2.1.0",
    "lint-staged": "11.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.3.1",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "type-coverage": "2.17.5",
    "typescript": "4.3.3",
    "typescript-coverage-report": "0.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn test:lint:fix"
  },
  "typeCoverage": {
    "atLeast": 80,
    "ignoreFiles": [
      "cypress/**/*",
      "tests/**/*"
    ]
  },
  "dependencies": {
    "compression": "1.7.4",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1"
  }
}
